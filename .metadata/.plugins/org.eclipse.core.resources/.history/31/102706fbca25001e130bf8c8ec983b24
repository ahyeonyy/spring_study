package com.example.demo.entity;

import jakarta.persistence.Entity;
import jakarta.persistence.Table;
import lombok.Data;

@Entity
@Data
@Table(name = "reply")
public class Reply {
	private int cno;
	private String comment;
	private String writer;
	private String regdate;
	private int b_ref;
	private int b_level;
	private int b_step;
}
